<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.Laform.mapper.productKeywordMapper">

<select id="getProductKeyword" resultType="com.Laform.entity.tb_product_keyword">
select * from tb_product_keyword where prod_idx=#{prod_idx} order by pr_sum desc limit 5
</select>

</mapper>